   <children>
      <TextField fx:id="TextField_lineNumberNewMap" layoutX="117.0" layoutY="118.0" />
      <TextField fx:id="TextField_columnNumberNewMap" layoutX="117.0" layoutY="188.0" />
      <Label layoutX="117.0" layoutY="171.0" text="Column number :" />
      <Label layoutX="117.0" layoutY="101.0" text="Line number :" />
      <Button layoutX="117.0" layoutY="239.0" mnemonicParsing="false" onAction="#handleNewMapButtonAction" text="Create a new map..." />
      <Label layoutX="117.0" layoutY="57.0" text="Set the parameters of your new map :">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
